<!DOCTYPE html>
<html>
<head>
  <title>RfpManager</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= ajaxful_rating_style %>
</head>
<body>
	
	<%= render 'layouts/header' %>
	<%= render 'layouts/menu' %>
	
	<section>
      
      <% flash.each do |key, value| -%>
        <%= content_tag(:div, value, :class => "flash #{key}") %>
      <% end -%>
      
      <%= yield %>
    </section>

</body>
</html>
